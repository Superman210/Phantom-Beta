{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "$": true,
    "moment": true,
    "authData": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    // "semi": [
    //   "error", 
    //   "always"
    // ],
    "strict": [
      "error"
    ]
  }
}